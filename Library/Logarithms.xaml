<Window x:Class="Enginer_Calc.Logarithms"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Enginer_Calc"
        mc:Ignorable="d"
        Title="Logarithms" Height="550" Width="750">
    <Grid>
        <Label Content="Тело" HorizontalAlignment="Left" Margin="173,66,0,0" VerticalAlignment="Top" Width="36" Height="24"/>
        <TextBox HorizontalAlignment="Left" Margin="132,104,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Height="34" FontSize="20" x:Name="tbA"/>
        <TextBox HorizontalAlignment="Left" Margin="305,104,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Height="34" FontSize="20" x:Name="tbB"/>
        <Label Content="Основание" HorizontalAlignment="Left" Margin="327,66,0,0" VerticalAlignment="Top" Width="76" Height="24"/>
        <TextBox HorizontalAlignment="Left" Margin="500,104,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Height="34" FontSize="20" IsReadOnly="True" x:Name="resultat"/>
        <Label Content="Результат" HorizontalAlignment="Left" Margin="528,66,0,0" VerticalAlignment="Top" Width="64" Height="33"/>
        <Button Content="Логарифм с основанием" HorizontalAlignment="Left" Margin="77,183,0,0" VerticalAlignment="Top" Height="45" Width="161" Background="#19FEDD" Click="Log_a"/>
        <Button Content="Логарифм с основанием 10" HorizontalAlignment="Left" Margin="284,183,0,0" VerticalAlignment="Top" Height="45" Width="162" Background="#19FEDD" Click="Log_10"/>
        <Button Content="Логарифм с основанием Exp" HorizontalAlignment="Left" Margin="485,183,0,0" VerticalAlignment="Top" Height="45" Width="167" Background="#19FEDD" Click="Log_e"/>

    </Grid>
</Window>
